@article{Lyd.zba2020,
  title = {Eigenstate {{Entanglement Entropy}} in {{Random Quadratic Hamiltonians}}},
  author = {\textbackslash Lyd\textbackslash.zba, Patrycja and Rigol, Marcos and Vidmar, Lev},
  date = {2020-10},
  journaltitle = {Phys. Rev. Lett.},
