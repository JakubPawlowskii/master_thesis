  author = {Scholl, P. and Williams, H. J. and Bornet, G. and Wallner, F. and Barredo, D. and Lahaye, T. and Browaeys, A. and Henriet, L. and Signoles, A. and Hainaut, C. and Franz, T. and Geier, S. and Tebben, A. and Salzinger, A. and Z\"urn, G. and Weidem\"uller, M.},
