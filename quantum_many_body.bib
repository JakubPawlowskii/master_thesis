@article{Kohn1959,
  title = {Theory of {{Bloch Electrons}} in a {{Magnetic Field}}: {{The Effective Hamiltonian}}},
  shorttitle = {Theory of {{Bloch Electrons}} in a {{Magnetic Field}}},
  author = {Kohn, Walter},
  date = {1959-09-15},
  journaltitle = {Phys. Rev.},
  volume = {115},
  number = {6},
  pages = {1460--1478},
  publisher = {{American Physical Society}},
  doi = {10.1103/PhysRev.115.1460},
  urldate = {2023-06-15},
  abstract = {The Hamiltonian of a Bloch electron in a static magnetic field is H=12P2+V(r), where V(r) is the periodic potential, P=p+Ac, and A is the vector potential giving rise to the magnetic field H. We consider the case of a nondegenerate band m. It is then shown that, with an error vanishing with H like HN+1 (N arbitrary), the eigenstates of H can be calculated from an equivalent Hamiltonian \textasciimacron Hm(P) with the following properties: (1) It is a one-band Hamiltonian, obtained by transforming away all relevant interband matrix elements. (2) It depends only on the gauge-covariant operators P{$\alpha$}. (3) It has the periodicity property \textasciimacron Hm(P+K)=\textasciimacron Hm(P), where K is an arbitrary reciprocal lattice vector. (4) It can be written as a series \textasciimacron Hm(P)={$\Sigma$}i=0Nsi\textasciimacron Hm;i(P) where s{$\equiv$}Hc and the functions \textasciimacron Hm;i(P) are completely symmetrized in the noncommuting operators P{$\alpha$}. Properties (3) and (4) can also be summarized in the equations \textasciimacron Hm(P)={$\Sigma$}la(l)\texttimes exp[iR(l){$\cdot$}P], where the R(l) are lattice vectors and the a(l) can be expanded as a(l)={$\Sigma$}i=0Nsiai(l). An algorithm is given for the construction of the \textasciimacron Hm;i and carried through for i=0, 1, 2. The formalism is not restricted to the neighborhood of the bottom and top of the band. We believe that the equivalent Hamiltonian \textasciimacron Hm(P) provides a sound basis for a discussion of wave functions and energy levels of Bloch electrons in a magnetic field.},
  file = {/home/jakubp/Documents/Research/NIQS/papers/1959_Kohn_Theory of Bloch Electrons in a Magnetic Field - The Effective Hamiltonian.pdf;/home/jakubp/Documents/Zotero/storage/765W4FR3/PhysRev.115.html}
