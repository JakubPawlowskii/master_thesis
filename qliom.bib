@Article{Suntajs2022,
  author        = {{\v{S}}untajs, Jan and Vidmar, Lev},
  date          = {2022},
  title         = {{Ergodicity breaking transition in zero dimensions}},
  eprint        = {2203.08844},
  eprinttype    = {arxiv},
  number        = {1},
  abstract      = {It is of great current interest to establish toy models of ergodicity breaking transitions in quantum many-body systems. Here we study a model dubbed quantum sun model that is expected to exhibit an ergodic to nonergodic transition in the thermodynamic limit upon tuning the coupling between an ergodic quantum dot and distant particles with spin-1/2. The model is effectively zero dimensional, however, a variant of the model is often referred to as the avalanche model of ergodicity breaking transition in one-dimensional disordered spin chains. We show that exact numerical results based on the spectral form factor calculation accurately agree with theoretical predictions, and can hence unambiguously confirm existence of the ergodicity breaking transition in this model. We benchmark specific properties that represent hallmarks of the ergodicity breaking transition in finite systems.},
  archiveprefix = {arXiv},
  arxivid       = {2203.08844},
  file          = {:Suntajs2022 - Ergodicity Breaking Transition in Zero Dimensions.pdf:PDF},
}

