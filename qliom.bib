@Article{Santos2010_2,
  author        = {Santos, Lea F. and Rigol, Marcos},
  date          = {2010},
  journaltitle  = {Physical Review E - Statistical, Nonlinear, and Soft Matter Physics},
  title         = {{Onset of quantum chaos in one-dimensional bosonic and fermionic systems and its relation to thermalization}},
