@Article{Prosen2014,
  author        = {Prosen, Toma{\v{z}}},
  date          = {2014-09},
  journaltitle  = {Nuclear Physics B},
  title         = {{Quasilocal conservation laws in XXZ spin-1/2 chains: Open, periodic and twisted boundary conditions}},
  doi           = {10.1016/j.nuclphysb.2014.07.024},
  eprint        = {1406.2258},
