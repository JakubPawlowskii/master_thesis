@Article{CamposVenuti2010,
  author       = {Campos Venuti, Lorenzo and Zanardi, Paolo},
  date         = {2010-02},
  journaltitle = {Physical Review A},
  title        = {Unitary equilibrations: {Probability} distribution of the {Loschmidt} echo},
  doi          = {10.1103/PhysRevA.81.022113},
  number       = {2},
  pages        = {022113},
  url          = {https://link.aps.org/doi/10.1103/PhysRevA.81.022113},
  urldate      = {2022-12-21},
  volume       = {81},
  abstract     = {Closed quantum systems evolve unitarily and therefore cannot converge in a strong sense to an equilibrium state starting out from a generic pure state. Nevertheless for large system size one observes temporal typicality. Namely, for the overwhelming majority of the time instants, the statistics of observables is practically indistinguishable from an effective equilibrium one. In this paper we consider the Loschmidt echo (LE) to study this sort of unitary equilibration after a quench. We draw several conclusions on general grounds and on the basis of an exactly solvable example of a quasifree system. In particular we focus on the whole probability distribution of observing a given value of the LE after waiting a long time. Depending on the interplay between the initial state and the quench Hamiltonian, we find different regimes reflecting different equilibration dynamics. When the perturbation is small and the system is away from criticality the probability distribution is Gaussian. However close to criticality the distribution function approaches a double-peaked, universal form.},
  file         = {Full Text PDF:https\://journals.aps.org/pra/pdf/10.1103/PhysRevA.81.022113:application/pdf},
  publisher    = {American Physical Society},
  shorttitle   = {Unitary equilibrations},
}

