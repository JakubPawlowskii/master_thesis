@Article{Polkovnikov2011863,
  author        = {Polkovnikov, Anatoli and Sengupta, Krishnendu and Silva, Alessandro and Vengalattore, Mukund},
  date          = {2011},
  journaltitle  = {Reviews of Modern Physics},
  title         = {{Colloquium: Nonequilibrium dynamics of closed interacting quantum systems}},
  doi           = {10.1103/RevModPhys.83.863},
  issn          = {0034-6861},
  number        = {3},
  pages         = {863--883},
  volume        = {83},
  abstract      = {This Colloquium gives an overview of recent theoretical and experimental progress in the area of nonequilibrium dynamics of isolated quantum systems. There is particularly a focus on quantum quenches: the temporal evolution following a sudden or slow change of the coupling constants of the system Hamiltonian. Several aspects of the slow dynamics in driven systems are discussed and the universality of such dynamics in gapless systems with specific focus on dynamics near continuous quantum phase transitions is emphasized. Recent progress on understanding thermalization in closed systems through the eigenstate thermalization hypothesis is also reviewed and relaxation in integrable systems is discussed. Finally key experiments probing quantum dynamics in cold atom systems are overviewed and put into the context of our current theoretical understanding. {\textcopyright} 2011 American Physical Society.},
