@Article{Basov2011471,
  author        = {Basov, D. N. and Averitt, Richard D. and {Van Der Marel}, Dirk and Dressel, Martin and Haule, Kristjan},
  date          = {2011},
  journaltitle  = {Reviews of Modern Physics},
  title         = {{Electrodynamics of correlated electron materials}},
