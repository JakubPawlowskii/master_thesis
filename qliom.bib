@Article{Moudgalya2022,
  author       = {Moudgalya, Sanjay and Bernevig, B. Andrei and Regnault, Nicolas},
  date         = {2022-07},
  journaltitle = {Reports on Progress in Physics},
  title        = {Quantum many-body scars and {Hilbert} space fragmentation: a review of exact results},
