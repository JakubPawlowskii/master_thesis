@Article{Xia2015316,
  author        = {Xia, Lin and Zundel, Laura A. and Carrasquilla, Juan and Reinhard, Aaron and Wilson, Joshua M. and Rigol, Marcos and Weiss, David S.},
  date          = {2015},
  journaltitle  = {Nature Physics},
  title         = {{Quantum distillation and confinement of vacancies in a doublon sea}},
  doi           = {10.1038/nphys3244},
  eprint        = {1409.2882},
  eprinttype    = {arXiv},
  issn          = {1745-2481},
  number        = {4},
  pages         = {316--320},
  volume        = {11},
  abstract      = {Ultracold atomic gases have revolutionized the study of non-equilibrium dynamics in quantum many-body systems. Many counterintuitive non-equilibrium effects have been observed, such as suppressed thermalization in a one-dimensional (1D) gas, the formation of repulsive self-bound dimers, and identical behaviours for attractive and repulsive interactions. Here, we observe the expansion of a bundle of ultracold 1D Bose gases in a flat-bottomed optical lattice potential. By combining in situ measurements with photoassociation, we follow the spatial dynamics of singly, doubly and triply occupied lattice sites. The system sheds interaction energy by dissolving some doublons and triplons. Some singlons quantum distil out of the doublon centre, whereas others remain confined. Our Gutzwiller mean-field model captures these experimental features in a physically clear way. These experiments might be used to study thermalization in systems with particle losses, the evolution of quantum entanglement or, if applied to fermions, to prepare very low entropy states.},
  annotation    = {From Duplicate 1 (Quantum distillation and confinement of vacancies in a doublon sea - Xia, Lin; Zundel, Laura A.; Carrasquilla, Juan; Reinhard, Aaron; Wilson, Joshua M.; Rigol, Marcos; Weiss, David S.) From Duplicate 2 (Quantum distillation and confinement of vacancies in a doublon sea - Xia, Lin; Zundel, Laura A.; Carrasquilla, Juan; Reinhard, Aaron; Wilson, Joshua M.; Rigol, Marcos; Weiss, David S.) And Duplicate 3 (Quantum distillation and confinement of vacancies in a doublon sea - Xia, L; Zundel, L A; Carrasquilla, J; Reinhard, A; Wilson, J M; Rigol, M; Weiss, D S) cited By 33 From Duplicate 2 (Quantum distillation and confinement of vacancies in a doublon sea - Xia, Lin; Zundel, Laura A.; Carrasquilla, Juan; Reinhard, Aaron; Wilson, Joshua M.; Rigol, Marcos; Weiss, David S.) cited By 33},
