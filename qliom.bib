  date         = {2014-09},
  journaltitle = {Physical Review Letters},
  title        = {Quenching the {Anisotropic} {Heisenberg} {Chain}: {Exact} {Solution} and {Generalized} {Gibbs} {Ensemble} {Predictions}},
  doi          = {10.1103/PhysRevLett.113.117202},
  number       = {11},
  pages        = {117202},
  url          = {https://link.aps.org/doi/10.1103/PhysRevLett.113.117202},
  urldate      = {2022-12-18},
  volume       = {113},
  abstract     = {We study quenches in integrable spin-1/2 chains in which we evolve the ground state of the antiferromagnetic Ising model with the anisotropic Heisenberg Hamiltonian. For this nontrivially interacting situation, an application of the first-principles-based quench-action method allows us to give an exact description of the postquench steady state in the thermodynamic limit. We show that a generalized Gibbs ensemble, implemented using all known local conserved charges, fails to reproduce the exact quench-action steady state and to correctly predict postquench equilibrium expectation values of physical observables. This is supported by numerical linked-cluster calculations within the diagonal ensemble in the thermodynamic limit.},
  file         = {Full Text PDF:https\://journals.aps.org/prl/pdf/10.1103/PhysRevLett.113.117202:application/pdf},
  publisher    = {American Physical Society},
  shorttitle   = {Quenching the {Anisotropic} {Heisenberg} {Chain}},
}

@Article{Lange2017,
  author       = {Florian Lange and Zala Lenar{\v{c}}i{\v{c}} and Achim Rosch},
  date         = {2017-06-09},
  journaltitle = {Nature Communications},
  title        = {Pumping approximately integrable systems},
  doi          = {10.1038/ncomms15767},
  number       = {1},
  volume       = {8},
  abstract     = {Weak perturbations can drive an interacting many-particle system far from its initial equilibrium state if one is able to pump into degrees of freedom approximately protected by conservation laws. This concept has for example been used to realize Bose-Einstein condensates of photons, magnons and excitons. Integrable quantum systems, like the one-dimensional Heisenberg model, are characterized by an infinite set of conservation laws. Here, we develop a theory of weakly driven integrable systems and show that pumping can induce large spin or heat currents even in the presence of integrability breaking perturbations, since it activates local and quasi-local approximate conserved quantities. The resulting steady state is qualitatively captured by a truncated generalized Gibbs ensemble with Lagrange parameters that depend on the structure but not on the overall amplitude of perturbations nor the initial state. We suggest to use spin-chain materials driven by terahertz radiation to realize integrability-based spin and heat pumps.},
  day          = {9},
  file         = {:quantum_many_body/papers/Lange2017 - Pumping Approximately Integrable Systems.pdf:PDF},
  month        = {jun},
  publisher    = {Springer Science and Business Media {LLC}},
  rights       = {Â© 2017 The Author(s)},
  year         = {2017},
}

